import mx.controls.TextInput;import mx.controls.Button;import mx.utils.Delegate;import gfx.launchpanel.views.BaseDialog;//import com.scaleform.launchpanel.views.BaseDialog;class gfx.launchpanel.views.PreferencesDialog extends BaseDialog {		// Constants:		public static var CLASS_REF = gfx.launchpanel.views.PreferencesDialog;	public static var LINKAGE_ID:String = "PreferencesDialog";	// Public Properties:		// Private Properties:	//private var commands:String; // From SO	private var _command:String;		// UI Elements:	private var commandsInput:TextInput;	private var submitBtn:Button;	private var cancelBtn:Button;	// Initialization:	private function PreferencesDialog() { super(); }	// Public Methods:		// Private Methods:	private function configUI():Void {		super.configUI();		submitBtn.addEventListener("click", Delegate.create(this, submit));		cancelBtn.addEventListener("click", Delegate.create(this, close));		commandsInput.text = _command;	}	public function set commands(p_command:String):Void {		_command = p_command;	}	private function getSubmitData():Object {		return {			commands: commandsInput.text		};	}}