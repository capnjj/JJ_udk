import mx.controls.TextInput;import mx.controls.Button;import mx.utils.Delegate;import gfx.launchpanel.views.BaseDialog;class gfx.launchpanel.views.AlertDialog extends BaseDialog {		// Constants:		public static var CLASS_REF = gfx.launchpanel.views.AlertDialog;	public static var LINKAGE_ID:String = "AlertDialog";	// Public Properties:		// Private Properties:	private var message:String		// UI Elements:	private var messageInput:TextField;	private var submitBtn:Button;	private var cancelBtn:Button;	// Initialization:	private function AlertDialog() { super(); }	// Public Methods:		// Private Methods:	private function configUI():Void {		super.configUI();		messageInput.text = message;		cancelBtn.addEventListener("click", Delegate.create(this, close));		submitBtn.addEventListener("click", Delegate.create(this, onRemove));	}		private function onRemove(eventObj:Object):Void {		close();	}}