import mx.events.EventDispatcher;class gfx.launchpanel.views.BaseDialog extends MovieClip {		// Constants:		public static var CLASS_REF = gfx.launchpanel.views.BaseDialog;	public static var LINKAGE_ID:String = "BaseDialog";	// Public Properties:	public var addEventListener:Function;	public var removeEventListener:Function;	private var dispatchEvent:Function;		// Private Properties:	private var title:String;		// UI Elements:	private var titleField:TextField;	// Initialization:	private function BaseDialog() {		EventDispatcher.initialize(this);	}		private function onLoad():Void { configUI(); }	// Public Methods:		// Private Methods:	private function configUI():Void {		titleField.text = title || "Dialog";	}	private function close(p_evtObj:Object):Void {		dispatchEvent({type:"close"});	}	private function submit(p_evtObj:Object):Void {		if (!validate()) { return; }		dispatchEvent({type:"submit", data:getSubmitData()});	}	private function validate():Boolean {		return true;	}	private function getSubmitData():Object {		return null;	}}